Link: https://leetcode.com/problems/binary-prefix-divisible-by-5/description/?envType=daily-question&envId=2025-11-24

class Solution {
public:
    vector<bool> prefixesDivBy5(vector<int>& nums) {
        int val = 0;
        vector<bool>res;
        for(auto &n:nums){
            val = ((val<<1) + n)%5;
            res.push_back(val==0);
        }
        return res;
    }
};
